{
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@hat-open/util": "^0.6.6",
    "snabbdom": "^3.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src_js/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "preset": "ts-jest",
    "roots": [
      "test_jest"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "\\.js$": [
        "babel-jest",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "node": "current"
                }
              }
            ]
          ]
        }
      ]
    },
    "transformIgnorePatterns": []
  }
}
